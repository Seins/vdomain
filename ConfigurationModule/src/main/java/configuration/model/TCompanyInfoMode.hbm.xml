<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.TCompanyInfoMode" table="t_company_info" schema="" catalog="vdomain">
        <id name="id">
            <column name="ID" sql-type="int" not-null="true"/>
        </id>
        <property name="infoTitle">
            <column name="INFO_TITLE" sql-type="varchar" length="100"/>
        </property>
        <property name="infoDesc">
            <column name="INFO_DESC" sql-type="varchar" length="2000"/>
        </property>
        <property name="infoImageUrl">
            <column name="INFO_IMAGE_URL" sql-type="varchar" length="200"/>
        </property>
    </class>
</hibernate-mapping>