<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.TMerchantsFuncModel" table="t_merchants_func" schema="" catalog="vdomain">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="merchantName">
            <column name="merchantName" sql-type="varchar" length="70"/>
        </property>
        <property name="merchantPicUrl">
            <column name="merchantPicUrl" sql-type="varchar" length="500"/>
        </property>
        <property name="merchantDesc">
            <column name="merchantDesc" sql-type="varchar" length="500"/>
        </property>
    </class>
</hibernate-mapping>