<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.TAboutUsModel" table="t_about_us" schema="" catalog="vdomain">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="title">
            <column name="title" sql-type="varchar" length="45"/>
        </property>
        <property name="imgUrl">
            <column name="imgUrl" sql-type="varchar" length="500"/>
        </property>
        <property name="desc">
            <column name="desc" sql-type="varchar" length="1000"/>
        </property>
    </class>
</hibernate-mapping>