<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.TSuccessfulCaseModel" table="t_successful_case" schema="" catalog="vdomain">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="caseName">
            <column name="caseName" sql-type="varchar" length="70"/>
        </property>
        <property name="caseTitle">
            <column name="caseTitle" sql-type="varchar" length="70"/>
        </property>
        <property name="caseDesc">
            <column name="caseDesc" sql-type="varchar" length="1000"/>
        </property>
        <property name="caseImgUrl">
            <column name="caseImgUrl" sql-type="varchar" length="500"/>
        </property>
        <property name="caseGuideUrl">
            <column name="caseGuideUrl" sql-type="varchar" length="100"/>
        </property>
    </class>
</hibernate-mapping>